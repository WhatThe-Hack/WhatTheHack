{
  "name": "whatthehack",
  "version": "1.0.0",
  "description": "A cyber-sec tool to be used responsibly in identifying XSS vulnerabilities",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/WhatThe-Hack/WhatTheHack.git"
  },
  "author": {
    "name": "Jason Yoon, Michael Geismar, Tommy Edmunds",
    "email": "jasony779@gmail.com",
    "url": "https://github.com/WhatThe-Hack/WhatTheHack"
  },
  "keywords": [
    "app",
    "boilerplate",
    "electron",
    "open",
    "open-source",
    "postcss",
    "react",
    "reactjs",
    "source",
    "webpack"
  ],
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "browserslist": [
    "last 4 versions"
  ],
  "main": "main.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "cross-env NODE_ENV=development webpack serve --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.build.config.js --mode production",
    "dev": "concurrently \"cross-env NODE_ENV=development --config=./webpack.dev.config.js\" \"NODE_ENV=development webpack serve --open\"",
    "package": "npm run build",
    "postpackage": "electron-packager ./ --out=./builds"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "@types/react-transition-group": "^4.4.2",
    "concurrently": "^6.2.0",
    "electron-storage": "^1.0.7",
    "electron-store": "^8.0.0",
    "jsdom": "^16.6.0",
    "nodemon": "^2.0.12",
    "postcss": "^8.2.8",
    "puppeteer-core": "^10.1.0",
    "puppeteer-in-electron": "^3.0.5",
    "react": "^17.0.1",
    "react-axios": "^2.0.5",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.1",
    "electron": "^12.0.15",
    "electron-devtools-installer": "^3.1.1",
    "electron-packager": "^15.2.0",
    "electron-reloader": "^1.2.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.3.9",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^5.1.0",
    "postcss-nested": "^5.0.5",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^5.1.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
